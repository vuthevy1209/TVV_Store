<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>

<body>
    {{{body}}}

    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>

    {{#if error}}
        <script>
            //# sourceURL=auth.js
            swal("Error", "{{error.[0]}}", "error");
        </script>
    {{/if}}

    {{#if success}}
        <script>
            //# sourceURL=auth.js
            swal("Success", "{{success.[0]}}", "success");
        </script>
    {{/if}}

</body>

</html>